{
  "name": "cypress-contract",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "dependencies": {
    "cypress": "^9.5.1",
    "mocha-junit-reporter": "^2.0.2",
    "wait-on": "^6.0.1"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.26.5",
    "allure-commandline": "^2.17.2",
    "mocha-allure-reporter": "^1.4.0"
  },
  "scripts": {
    "test": "npm run cy:run || npm run posttest",
    "cy:run": "cypress run --record --key 0c2a89c2-8fa0-4037-ac2d-5451633fb8ca --parallel --reporter mocha-allure-reporter",
    "allure:report": "allure generate  allure-results --clean -o allure-report && allure open allure-report",
    "allure:clear": "rm -rf allure-results && rm -rf allure-report rm -rf cypress/videos && rm -rf cypress/screenshots",
    "pretest": "npm run allure:clear",
    "posttest": "npm run allure:report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rauru/deel-assesment.git"
  },
